{
    "name": "wallet-backend",
    "description": "Rabbit Wallet Application Backend",
    "main": "index.js",
    "private": true,
    "type": "module",
    "scripts": {
        "build": "next build",
        "provide-prod-properties": "cross-env-shell PROP_ENV=prod \"node ../properties/provideProperties.mjs\"",
        "provide-dev-properties": "cross-env-shell PROP_ENV=dev \"node ../properties/provideProperties.mjs\"",
        "provide-local-properties": "cross-env-shell PROP_ENV=local \"node ../properties/provideProperties.mjs\"",
        "provide-unit_tests-properties": "cross-env-shell PROP_ENV=unit_tests \"node ../properties/provideProperties.mjs\"",
        "provide-integration_tests-properties": "cross-env-shell PROP_ENV=integration_tests \"node ../properties/provideProperties.mjs\"",
        "server-prod": "npm run provide-prod-properties && NODE_ENV=production node ./src/index.js",
        "server-dev": "npm run provide-dev-properties && node ./src/index.js",
        "server-local": "npm run provide-local-properties && npm run build && PROP_ENV=local nodemon ./src/index.js",
        "server-local-plus-unreleased-dev-ui-kit": "rm -rf .next ./node_modules/@rabbitio/ui-kit/src ./node_modules/@rabbitio/ui-kit/dist ./node_modules/@rabbitio/ui-kit/index.js && cp -r $UI_KIT_PATH/src $UI_KIT_PATH/dist $UI_KIT_PATH/index.js ./node_modules/@rabbitio/ui-kit/ && npm run provide-local-properties && npm run build && PROP_ENV=local nodemon ./src/index.js",
        "server-unit_tests": "npm run provide-unit_tests-properties && npm run build && node ./src/index.js",
        "server-integration_tests": "npm run provide-integration_tests-properties && node ./src/index.js",
        "validate-translations": "node ../scripts/validate-translations.mjs"
    },
    "browserslist": [],
    "repository": {
        "type": "git",
        "url": "git+https://gitlab.com/rabbitio/wallet.git"
    },
    "keywords": [
        "Bitcoin",
        "BTC",
        "wallet",
        "rabbit",
        "segwit"
    ],
    "author": "Rabbit Ltd.",
    "bugs": {
        "url": "https://gitlab.com/rabbitio/wallet/issues"
    },
    "homepage": "https://rabbit.io",
    "devDependencies": {
        "@babel/register": "7.16.9",
        "babel-loader": "8.2.3",
        "babel-polyfill": "6.26.0",
        "babel-preset-env": "1.7.0",
        "chalk": "^5.5.0",
        "copy": "0.0.1",
        "cross-env": "7.0.3",
        "fs-extra": "8.1.0",
        "mocha": "10.3.0",
        "mochawesome": "7.0.1",
        "nodemon": "3.0.3",
        "should": "13.2.3",
        "sinon": "7.5.0"
    },
    "dependencies": {
        "@emotion/react": "^11.14.0",
        "@emotion/styled": "^11.14.0",
        "@mui/icons-material": "^7.0.2",
        "@mui/joy": "^5.0.0-beta.52",
        "@mui/material": "^7.0.2",
        "@mui/x-data-grid": "^7.28.3",
        "@rabbitio/ui-kit": "1.0.0-beta.113",
        "axios": "1.6.7",
        "body-parser": "1.20.2",
        "bottleneck": "2.19.5",
        "cookie-parser": "1.4.6",
        "express": "4.18.2",
        "fast-json-stable-stringify": "2.1.0",
        "fs": "0.0.1-security",
        "html-to-text": "9.0.5",
        "http-proxy-middleware": "2.0.6",
        "joi": "17.12.1",
        "jshashes": "1.0.8",
        "l": "0.6.0",
        "log4js": "6.9.1",
        "mongodb": "3.7.4",
        "mongodb-core": "3.2.7",
        "morgan": "1.10.0",
        "multer": "1.4.4",
        "next": "14.1.0",
        "next-i18next": "15.2.0",
        "node-schedule": "2.1.0",
        "nodemailer": "6.9.9",
        "path": "0.12.7",
        "pbkdf2": "3.1.2",
        "promise-retry": "1.1.1",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-flickity-component": "4.0.7",
        "react-transition-group": "4.4.5",
        "util": "0.11.1",
        "uuid": "9.0.0"
    }
}
